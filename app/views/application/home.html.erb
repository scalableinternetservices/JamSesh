<div class="container">
  <h1>Hello, World!</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Email</th>
        <th>Location</th>
        <th>Instruments</th>
        <th>Date Joined</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.profile.location %></td>
          <td>
          <% user.profile.instrument.each do |instrument| %>
            <%= instrument.name %>
            <% end %>
          </td>
          <td><%= user.created_at.to_date.to_formatted_s(:rfc822) %></td>
          
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

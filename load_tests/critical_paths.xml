          content_type="application/x-www-form-urlencoded"></http>
      </request>
      <thinktime value="4" random="true"></thinktime>
  
      <!--Edit public profile-->
      <request subst="true">
  <dyn_variable name="authenticity_token"></dyn_variable>
        <http url='/profiles/%%_profileid%%/edit_profile' version='1.1' method='GET'></http>
      </request>
      <thinktime value="10" random="true"></thinktime>
  

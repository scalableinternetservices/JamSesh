<?xml version="1.0"?>
<!DOCTYPE tsung SYSTEM "/home/ec2-user/opt/tsung-1.5.0/share/tsung/tsung-1.0.dtd">
<tsung loglevel="notice" version="1.0">

  <!-- Client side setup -->
  <clients>
    <client host="localhost" use_controller_vm="true" maxusers='15000'/>
  </clients>

  <!-- Server side setup -->
  <servers>
    <server host="jamsesh-test.wxtcguxzgh.us-west-2.elasticbeanstalk.com" port="80" type="tcp"></server>
  </servers>


  <load>
    <arrivalphase phase="1" duration="5" unit="second">
      <users arrivalrate="1" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="2" duration="5" unit="second">
      <users arrivalrate="2" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="3" duration="5" unit="second">
      <users arrivalrate="3" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="4" duration="5" unit="second">
      <users arrivalrate="4" unit="second"></users>
    </arrivalphase>
    <arrivalphase phase="5" duration="5" unit="second">
      <users arrivalrate="5" unit="second"></users>
    </arrivalphase>
  </load>

  <options>
    <!-- Set connection timeout to 2 seconds -->
    <option name="global_ack_timeout" value="2000"></option>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>




<!-- RETURNING USER -->




  <sessions>
    <session name="returning_user" probability="30" type="ts_http">
      <!-- visit home page -->
      <request>
      	<http url='/' version='1.1' method='GET'></http>
      </request>
      <thinktime value="2" random="true"></thinktime>
      
      <!-- visit signin page-->
      <request>
        <dyn_variable name="authenticity_token"></dyn_variable>
      	<http url='/users/sign_in' version='1.1' method='GET'></http>
      </request>
      <thinktime value="7" random="true"></thinktime>
      
      <!-- choose random user -->
      <setdynvars sourcetype="random_number" start="1" end="200">
      <var name="usernumber" />
      </setdynvars>

      <!-- urlencode authenticity_token -->
      <setdynvars sourcetype="eval" 
      code='fun({Pid,DynVars})->
{ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars)
,re:replace(Val,"[+]","%2B",[global, {return,list}]) end.'>
        <var name="authenticity_token" />
      </setdynvars>
    
      <!-- sign in -->
      <request subst="true">
        <http url='/users/sign_in' version='1.1' method='POST'
          contents="authenticity_token=%%_authenticity_token%%&amp;user%5bemail%5d=person%%_usernumber%%%40example.com&amp;user%5Bpassword%5D=password"
          content_type="application/x-www-form-urlencoded"></http>
      </request>
      <thinktime value="6" random="true"></thinktime>

      <!-- Go to Jam Groups-->
      <request>
      	<http url='/jam_groups' version='1.1' method='GET'></http>
      </request>
      <thinktime value="7" random="true"></thinktime>

      <!-- Visit Jam Group-->
      <request subst="true">
        <dyn_variable name="groupid" re='&lt;a href="/jam_groups/(\d*)"&gt;'></dyn_variable>
      	<http url='/jam_groups/%%groupid%%' version='1.1' method='GET'></http>
      </request>
      <thinktime value="2" random="true"></thinktime>

      <!--Visit chat-->
      <request subst="true">
      <dyn_variable name="authenticity_token"></dyn_variable>
      	<http url='/jam_groups/%%groupid%%/chat' version='1.1' method='GET'></http>
      </request>
      <thinktime value="10" random="true"></thinktime>

      <!-- urlencode authenticity_token -->
      <setdynvars sourcetype="eval" 
                  code='fun({Pid,DynVars})->
                        {ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars)
                        ,re:replace(Val,"[+]","%2B",[global, {return,list}]) end.'>
        <var name="authenticity_token" />
      </setdynvars>
      
      <!--Post Comment-->
      <request subst="true">
        <http url='/jam_groups/%%groupid%%/chat' version='1.1' method='POST'
      	  contents="authenticity_token=%%_authenticity_token%%&amp;comment[comment]=hi&amp;commit=Add Comment"
          content_type="application/x-www-form-urlencoded"></http>
      </request>
      <thinktime value="4" random="true"></thinktime>

    </session>
    
    
    
    
    
    
    <!-- NEW USER -->
    
    
    
    
    
  <?ignore
     <session name="new_user" probability="65" type="ts_http">
      <!-- visit home page -->
      <request>
      	<http url='/' version='1.1' method='GET'></http>
      </request>
      <thinktime value="2" random="true"></thinktime>
      

      <thinktime value="7" random="true"></thinktime>
      
      <!-- choose random user -->
      <setdynvars sourcetype="random_number" start="1" end="200">
      <var name="usernumber" />
      </setdynvars>
  
      <!-- urlencode authenticity_token -->
      <setdynvars sourcetype="eval" 
      code='fun({Pid,DynVars})->
  {ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars)
  ,re:replace(Val,"[+]","%2B",[global, {return,list}]) end.'>
        <var name="authenticity_token" />
      </setdynvars>
    
      <!-- sign in -->
      <request subst="true">
        <http url='/users/sign_in' version='1.1' method='POST'
          contents="authenticity_token=%%_authenticity_token%%&amp;user%5bemail%5d=person%%_usernumber%%%40example.com&amp;user%5Bpassword%5D=password"
          content_type="application/x-www-form-urlencoded"></http>
      </request>
      <thinktime value="6" random="true"></thinktime>
  
      <!--Go to edit profile page-->
      <request subst="true">
        <dyn_variable name="profileid" re='&lt;a href="/profiles/(\d*)/edit_profile"&gt;Edit Profile'></dyn_variable>
      	<http url='/profiles/%%_usernumber%%/edit_profile' version='1.1' method='GET'></http>
      </request>
      <thinktime value="2" random="true"></thinktime>
  
      <!-- validate profileid, or replace with usernumber -->
      <setdynvars sourcetype="eval" 
                  code='fun({Pid,DynVars})->
                        {ok,Val}=ts_dynvars:lookup(profileid,DynVars)
                        ,{ok,Val2}=ts_dynvars:lookup(usernumber,DynVars)
  		,re:replace(Val,"^$",Val2,[global, {return,list}]) end.'>
        <var name="profileid" />
      </setdynvars>
  
      <!--Edit contact info-->
      <request subst="true">
  <dyn_variable name="authenticity_token"></dyn_variable>
      	<http url='/profiles/%%_profileid%%/edit_contact' version='1.1' method='GET'></http>
      </request>
      <thinktime value="10" random="true"></thinktime>
  
      <!-- urlencode authenticity_token -->
      <setdynvars sourcetype="eval" 
                  code='fun({Pid,DynVars})->
                        {ok,Val}=ts_dynvars:lookup(authenticity_token,DynVars)
                        ,re:replace(Val,"[+]","%2B",[global, {return,list}]) end.'>
        <var name="authenticity_token" />
      </setdynvars>
      
      <request subst="true">
        <http url='/profiles/%%_profileid%%' version='1.1' method='POST'

        <var name="authenticity_token" />
      </setdynvars>
  
      <request subst="true">
        <http url='/profiles/%%_profileid%%' version='1.1' method='POST'
          contents="authenticity_token=%%_authenticity_token%%&amp;_method=patch&amp;profile%5bfirst_name%5d=person%%_usernumber%%&amp;user%5Blast_name%5D=Clinton&amp;profile%5bage%5d=69&amp;profile%5blocation%5d=SantaBarbara&amp;profile%5bartists_list%5d=&amp;profile%5bbio%5d=iLikeCheeseAndMusak"
          content_type="application/x-www-form-urlencoded"></http>
